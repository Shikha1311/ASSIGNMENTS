<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="question" class="Question">
		<property name="questionId">
			<list>
				<value>"1"</value>
				<value>"2"</value>
			</list>
		</property>
	   <property name = "question">
         <set>
            <value>"what is Java?"</value>
            <value>"what is Spring?"</value>

         </set>
      </property>

       <property name = "answers">
         <map>
			 <entry key="1" value="Java is a platform-independent, object-oriented programming language "/>
            <entry key = "2" value = "Spring is a lightweight framework. "/>
             
         </map>
	    </property>

	</bean>
</beans>
